{
  "name": "vite-plugin-electron-renderer",
  "version": "0.11.4",
  "description": "Support use Node.js API in Electron-Renderer",
  "main": "index.mjs",
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron-vite/vite-plugin-electron-renderer.git"
  },
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --emitDeclarationOnly && vite build",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "dependencies": {
    "lib-esm": "~0.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.16.11",
    "rollup": "^3.8.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-utils": "^0.3.6"
  },
  "files": [
    "types",
    "index.mjs",
    "index.js",
    "electron-renderer.js",
    "builtins"
  ],
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "renderer"
  ]
}
